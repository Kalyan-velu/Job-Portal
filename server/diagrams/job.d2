graph TD {
    A[API] --> B[Job Control]
    
    B --> C[Create Job]
    B --> D[GET /api/jobs/active]
    B --> E[GET /api/jobs/archived]

    C --> F[Job Creation Logic]
    C --> G[Response: Job Created]
    
    D --> H[Search: Optional Parameter]
    D --> I[Query: isArchived = false]
    D --> J[Return Active Jobs]
    
    E --> K[Search: Optional Parameter]
    E --> L[Query: isArchived = true]
    E --> M[Return Archived Jobs]

    style A fill:#f9f,stroke:#333,stroke-width:2px;
    style B fill:#ccf,stroke:#333,stroke-width:2px;
    style C fill:#9ff,stroke:#333,stroke-width:2px;
    style D fill:#ccf,stroke:#333,stroke-width:2px;
    style E fill:#ccf,stroke:#333,stroke-width:2px;
    style F fill:#eef,stroke:#333,stroke-width:2px;
    style G fill:#afa,stroke:#333,stroke-width:2px;
    style H fill:#eef,stroke:#333,stroke-width:2px;
    style I fill:#eef,stroke:#333,stroke-width:2px;
    style J fill:#afa,stroke:#333,stroke-width:2px;
    style K fill:#eef,stroke:#333,stroke-width:2px;
    style L fill:#eef,stroke:#333,stroke-width:2px;
    style M fill:#afa,stroke:#333,stroke-width:2px;
}
